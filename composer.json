{
    "name": "botman/botman",
    "license": "MIT",
    "description": "Create messaging bots in PHP with ease.",
    "keywords": [
        "Bot",
        "BotMan",
        "Chatbot"
    ],
    "homepage": "http://github.com/botman/botman",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/http-foundation": "^6.0",
        "tightenco/collect": "^9.0",
        "opis/closure": "^3.6",
        "mpociot/pipeline": "^1.0",
        "react/socket": "^1.11",
        "spatie/macroable": "^2.0",
        "psr/container": "^2.0"

    },
    "suggest": {
        "doctrine/cache": "Use any Doctrine cache driver for cache",
        "symfony/cache": "Use any Symfony cache driver for cache"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\BotMan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BotMan\\BotMan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        },
        "laravel": {
            "providers": [
                "BotMan\\BotMan\\BotManServiceProvider"
            ],
            "aliases": {
                "BotMan": "BotMan\\BotMan\\Facades\\BotMan"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "codeigniter/framework": "^3.1",
        "orchestra/testbench": "^7.0",
        "mockery/mockery": "^1.5",
        "doctrine/cache": "^2.1",
        "symfony/cache": "^6.0",
        "ext-curl": "*"
    }
}
